{
  "name": "vai-coxinha-backend",
  "version": "1.0.0",
  "description": "Backend API para Vai Coxinha - Lógica de negócios e APIs RESTful",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed": "tsx src/database/seed.ts",
    "db:clear:orders": "tsx src/utils/clearOrderHistory.ts",
    "mongo:start": "tsx src/app.ts",
    "mongo:dev": "tsx watch src/app.ts",
    "mongo:memory": "tsx src/app-memory.ts",
    "mongo:memory:dev": "tsx watch src/app-memory.ts",
    "docker:build": "docker build -t vai-coxinha-backend .",
    "docker:run": "docker run -p 3001:3001 vai-coxinha-backend"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/qrcode": "^1.5.5",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb-memory-server": "^9.1.3",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.7.0",
    "qrcode": "^1.5.4",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "api",
    "rest",
    "backend",
    "vai-coxinha",
    "food-delivery"
  ],
  "author": "Vai Coxinha Team",
  "license": "MIT"
}
